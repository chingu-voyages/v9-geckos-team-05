{
  "name": "Gecko 5",
  "description": "Gecko 5's productivity extension",
  "version": "1.0",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/js/content.js"]
    }
  ],
  "background": {
    "scripts": ["/js/background.js"]
  },
  "browser_action": {
    "default_popup": "index.html"
  }
}
